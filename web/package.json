{
  "private": true,
  "name": "blog-with-gatsby-web",
  "version": "1.0.7",
  "author": "maximsan maksim.shchytski@gmail.com",
  "scripts": {
    "build": "gatsby build",
    "clean-cache": "gatsby clean",
    "dev": "gatsby develop",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx}\" \"!.cache/**\" \"!node_modules/**\" \"!public/**\"",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "generate": "set DEBUG=1 && graphql-codegen --config codegen.yml",
    "generate:watch": "yarn generate -w",
    "generate:css": "tcm -p 'src/**/*.module.css' .",
    "generate:css:w": "yarn generate:css -w",
    "start": "concurrently --kill-others-on-fail \"yarn dev\" \"yarn generate:css:w\""
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.4.0",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.4.2",
    "@graphql-codegen/typescript-operations": "2.2.2",
    "concurrently": "^7.0.0",
    "dotenv": "^16.0.0",
    "postcss": "^8.4.6",
    "postcss-preset-env": "^7.3.1"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.6.0",
    "@mui/material": "^5.4.0",
    "@sanity/block-content-to-react": "^2.0.7",
    "@sanity/image-url": "^1.0.1",
    "@types/node": "^17.0.13",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-helmet": "^6.1.5",
    "date-fns": "^2.28.0",
    "gatsby": "^4.6.1",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-image": "^2.6.0",
    "gatsby-plugin-postcss": "^5.6.0",
    "gatsby-plugin-react-helmet": "^5.6.0",
    "gatsby-plugin-styled-components": "^5.6.0",
    "gatsby-source-sanity": "^7.3.2",
    "graphql": "^15.8.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0"
  }
}
